{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "andreslav/autogrid",
  "version": "0.1.0",
  "title": "Autogrid",
  "category": "design",
  "icon": "smiley",
  "description": "Autogrid Block. The elements inside this block are automatically distributed inÂ columns, filling the available space.",
  "example": {
    "attributes": {
      "columnCount": 2,
      "minWidth": 200
    }
  },
  "supports": {
    "html": false,
    "color": {
      "text": true,
      "background": true,
      "gradients": true,
      "__experimentalDefaultControls": {}
    },
    "spacing": {
      "padding": true,
      "margin": [
        "top",
        "bottom"
      ],
      "__experimentalDefaultControls": {
        "padding": false,
        "margin": false
      }
    },
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {}
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalTextTransform": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalDefaultControls": {}
    },
    "anchor": true,
    "align": [
      "wide",
      "full"
    ]
  },
  "attributes": {
    "columnCount": {
      "type": "number",
      "default": 4
    },
    "minWidth": {
      "type": "number",
      "default": 300
    },
    "gaps": {
      "type": "array",
      "default": [
        {
          "value": "var:preset|spacing|10",
          "min": "",
          "max": ""
        }
      ]
    },
    "childrenPaddings": {
      "type": "array",
      "default": [
        {
          "value": "var:preset|spacing|20",
          "min": "",
          "max": ""
        }
      ]
    }
  },
  "providesContext": {
    "autogrid/columnCount": "columnCount",
    "autogrid/minWidth": "minWidth"
  },
  "textdomain": "autogrid",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}