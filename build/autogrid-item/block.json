{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "andreslav/autogrid-item",
  "version": "0.1.0",
  "title": "Autogrid Item",
  "category": "design",
  "icon": "smiley",
  "description": "Autogrid Block Item.",
  "example": {},
  "supports": {
    "html": false,
    "color": {
      "text": true,
      "background": true
    },
    "spacing": {
      "padding": true,
      "margin": true,
      "__experimentalDefaultControls": {
        "padding": false,
        "margin": false
      }
    },
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {
        "color": false,
        "radius": false,
        "style": false,
        "width": false
      }
    }
  },
  "attributes": {
    "indexNode": {
      "type": "number",
      "default": 0
    },
    "sizes": {
      "type": "array",
      "default": []
    }
  },
  "parent": [
    "andreslav/autogrid"
  ],
  "usesContext": [
    "autogrid/columnCount",
    "autogrid/minWidth"
  ],
  "textdomain": "autogrid",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}